{% extends '@WebProfiler/Profiler/layout.html.twig' %}

{% block toolbar%}
    {% set icon %}
        <span>
            <img height="28" alt="Pomm" src="data:image/png;base64," />
            <span class="
                sf-toolbar-status
                {% if 50 < collector.querycount %} sf-toolbar-status-yellow{% endif %}
                {% if collector.exception is not null %} sf-toolbar-status-red{% endif %}
            ">{{ collector.querycount }}</span>
            {% if collector.querycount > 0 %}
                <span class="sf-toolbar-info-piece-additional-detail">in {{ '%0.2f'|format(collector.time) }} ms</span>
            {% endif %}
        </span>
    {% endset %}

    {% set text %}
        <div class="sf-toolbar-info-piece">
            <b>DB Queries</b>
            <span>{{ collector.querycount }}</span>
        </div>
        <div class="sf-toolbar-info-piece">
            <b>Query time</b>
            <span>{{ '%0.2f'|format(collector.time) }} ms</span>
        </div>
    {% endset %}

    {% include '@WebProfiler/Profiler/toolbar_item.html.twig' with { 'link': profiler_url } %}
{% endblock %}

{% block menu %}
    <span class="label">
        <span class="icon"><img height="28" alt="Pomm" src="data:image/png;base64," /></span>
        <strong>Pomm</strong>
        <span class="count">
            <span>{{ collector.querycount }}</span>
            <span>{{ '%0.0f'|format(collector.time) }} ms</span>
        </span>
    </span>
{% endblock %}


