{
    "version": "5.0.0",
    "name": "conserto/pomm-symfony-bridge",
    "type": "library",
    "description":  "Pomm profiler for Symfony.",
    "keywords": ["symfony","pomm", "debug", "profiler"],
    "homepage": "http://www.pomm-project.org",
    "license":  "MIT",
    "authors":  [ { "name": "Grégoire HUBERT", "email": "hubert.greg@gmail.com" }, { "name": "Jérôme MACIAS", "email": "jerome.macias@gmail.com" } ],
    "require": {
        "php": ">=8.4",
        "conserto/pomm-foundation": "4.0.x-dev as 4.0",
        "conserto/pomm-model-manager": "4.0.x-dev as 4.0",
        "symfony/http-foundation": "~7.0",
        "symfony/http-kernel": "~7.0",
        "symfony/serializer": "~7.0",
        "symfony/property-info": "~7.0",
        "symfony/stopwatch": "~7.0",
        "twig/twig": "~3.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "~3.7",
        "phpstan/phpstan": "^2.0",
        "rector/rector": "^2.0"
    },
    "suggest": {
        "conserto/pomm-cli": "4.0.x-dev as 4.0",
        "symfony/symfony": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "PommProject\\SymfonyBridge\\": "sources/lib"
        }
    },
    "minimum-stability": "stable"
}
